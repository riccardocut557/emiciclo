# Binance Futures API Keys
# Get yours at: https://www.binance.com/en/my/settings/api-management
# Enable "Futures" permission, disable "Withdrawal"
BINANCE_API_KEY=your_api_key_here
BINANCE_API_SECRET=your_api_secret_here

# Trading Configuration
SYMBOL=SUIUSDT
TIMEFRAME=1h
RANGE_MIN=13
RANGE_MAX=33
FORCE_24_BARS=true
ENABLE_LONG=true
ENABLE_SHORT=true

# Risk Management
LEVERAGE=20
CAPITAL_PERCENT=30

# Bot Settings
LOOP_INTERVAL_MS=60000
LOG_LEVEL=info

# Auto-Optimization: Recalculate best range every N hours
OPTIMIZE_INTERVAL_HOURS=32

# Use Binance Testnet for testing (true/false)
TESTNET=false
